#include<stdio.h>
#include<stdlib.h>
#include<math.h>
#include<string.h>

int main(void) {
    char s[100000];
    int i = 0, count = 1;
    scanf("%[^\n]%*c",&s);
    while(s[i] != '\0') {
        if (s[i] >= 'A' && s[i] <= 'Z') {
            count++;
        }
        i ++;
    }    
    printf("%d",count);
    return 0;
}
