Question :-  https://codeforces.com/problemset/problem/1/A

Solution : -


#include<bits/stdc++.h>
#define ll long long int
using namespace std;

int solve(ll n, ll m, ll a) {
    ll tile_length = (n % a == 0) ? n / a : n / a + 1;
    ll tile_width = (m % a == 0) ? m / a : m / a + 1;
    cout << tile_length * tile_width;
    return 0;
}

int main(void) {
    ios :: sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    
    ll n, m ,a;
    cin >> n >> m >> a;
    solve(n, m, a);
    return 0;
}
