Question :-     https://codeforces.com/problemset/problem/1519/B

Solution :-

#include<bits/stdc++.h>
using namespace std;
int main(void) {
    ios_base :: sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t, n, m , k;
    cin >> t;
    while (t --) {
        cin >> n >> m >> k;
        int cost = (m * (n - 1)) + (m - 1);
        if (cost == k) {
            cout << "YES\n";
        } else {
            cout << "NO\n";
        }
    }
    return 0;
}
