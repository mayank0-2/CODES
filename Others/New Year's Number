Question :-   https://codeforces.com/problemset/problem/1475/B

Solution :-  

#include<bits/stdc++.h>
using namespace std;
int check(int n) {
    if (n % 2020 == 0 || n % 2021 == 0) {
        cout << "YES" << "\n";
    } else if (n % 2020 <= n / 2020) {
        cout << "YES" << "\n";
    }  else {
        cout << "NO" << "\n";
    }
    return 0;
}
int main(void) {
    ios :: sync_with_stdio(0);
    cin.tie(0), cout.tie(0);

    int t;
    cin >> t;
    while(t --) {
        int n;
        cin >> n;
        check(n);
    }
    return 0;
}
