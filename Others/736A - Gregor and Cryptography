Problem:- https://codeforces.com/contest/1549/problem/A

Code:-

#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int solve (int p) {
    int a, b;
    a = p / 2;
    b = p - 1;
    cout << a << " " << b << "\n";
    return 0;
}
 
int main(void) {
    ios :: sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t;
    cin >> t;
    while (t --) {
        int p;
        cin >> p;
        solve(p);
    }
    return 0;
}
